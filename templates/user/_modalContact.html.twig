<div class="modal fade" id="T{{ club.id }}" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="exampleModalCenterTitle">Comment contacter ce club ?</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" style="overflow: auto;">
                <ul class="list-group list-group-flush">
                    <li class="list-group-item">{{ club.villeClub }} ({{ club.codePostalClub }})</li>
                    <li class="list-group-item">{{ club.emailClub }}</li>
                    <li class="list-group-item">{{ club.phoneClub }}</li>
                </ul>
            </div>
        </div>
    </div>
</div>

